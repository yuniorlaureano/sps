
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Error 403 | Berry - Admin.</title>


    <!--STYLESHEET-->
    <!--=================================================-->
    <!--Open Sans Font [ OPTIONAL ] -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700&amp;subset=latin" rel="stylesheet">

    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/nifty.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet" />


</head>

<!--TIPS-->
<!--You may remove all ID or Class names which contain "demo-", they are only used for demonstration. -->

<body>
    <div id="container" class="cls-container">

        <!-- HEADER -->
        <!--===================================================-->
        <div class="cls-header">
            <div class="cls-brand">
                <a class="box-inline" href="index.html">
                    <!-- <img alt="Nifty Admin" src="images/logo.png" class="brand-icon"> -->
                    <span class="brand-title">Berry <span class="text-thin">Admin</span></span>
                </a>
            </div>
        </div>

        <!-- CONTENT -->
        <!--===================================================-->
        <div class="cls-content">
            <h1 class="error-code text-warning">403</h1>
            <img src="@Url.Content("~/img/403.png")"></img>
            <p class="h4 text-thin pad-btm mar-btm">
                <i class="fa fa-exclamation-circle fa-fw" style="color: #bb7815"></i>
                Lo siento, usted no tiene permiso para acceder a esta página.
            </p>
            <div class="row mar-btm">
                <p class="h4 text-thin pad-btm mar-btm">
                    Favor contactar con el administrador del sistema.
                </p>
            </div>
            <br>
            <div class="pad-top"><a class="btn-link" href="~/">Regresar al inicio</a></div>
        </div>


    </div>

    <script src="@Url.Content("~/js/jquery-2.1.1.min.js")"></script>
    <script src="@Url.Content("~/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/plugins/fast-click/fastclick.min.js")"></script>
    <script src="@Url.Content("~/js/nifty.min.js")"></script>


</body>
</html>


