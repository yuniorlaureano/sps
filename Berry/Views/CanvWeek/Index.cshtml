
@{
    ViewBag.Title = "canv week";
}
<section class="featured">

    <div class="boxed">
        <div id="content-container">

            <div id="page-content">                
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-bordered" id="logReportPanel">
                            <div class="panel-heading">
                                <h3 class="panel-title">Weeklies abiertos</h3>
                                <div class="col-sm-12" style="padding-left:20px;">
                                    <div class="form-group">
                                        <label class="control-label" for="demo-hor-inputcanvass">Base de Datos:</label>
                                        <br />
                                        <label class="form-radio form-normal form-mint form-text"><input data-url="@Url.Action("GetOpenedWeek", "CanvWeek")" type="radio" class="chkDB" name="db" value="YP" checked>YP</label>
                                        <label class="form-radio form-normal form-mint form-text"><input data-url="@Url.Action("GetOpenedWeek", "CanvWeek")" type="radio" class="chkDB" name="db" value="BR">BR</label>
                                    </div>
                                </div>
                            </div>
                            <div class="panel-body" id="logReportPanel-body" data-toggle="panel-overlay" data-target="#logReportPanel-body">
                                <table id="tbLogReportPanel" data-url="@Url.Action("CloseWeek", "CanvWeek")" class="table table-hover table-bordered" width="100%">
                                    <thead>
                                        <tr>
                                            <th>Canvass week</th>
                                            <th>Calendar start date</th>
                                            <th>Calendar end date</th>
                                            <td></td>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>


@section scripts{
    <script src="@Url.Content("~/js/closeWeek.js")"></script>
}


