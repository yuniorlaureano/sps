
    <div id="demo-panel-ref" data-target="#demo-panel-ref" data-toggle="panel-overlay">
        <div class="panel-body">

            <div class="form-group">

                <label class="col-md-3 control-label" for="demo-hor-inputcanvass">ID Cliente:</label>
                <div class="col-md-4">
                    <input type="number" class="form-control" id="txtCustID" @*onkeypress="return justNumbers(event);"*@ name="integer">
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label" for="demo-hor-inputcanvass">Canvass:</label>
                <div class="col-lg-4">
                    <select class="selectpicker" data-url="@Url.Action("GetActiveCanvass", "CorrectionTrans")" id="ddlCanvCode" data-live-search="true" data-width="100%" data-style="btn-mint"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-3 control-label">Edición</label>
                <div class="col-lg-4">
                    <select class="selectpicker" id="ddlEdition" data-live-search="true" data-width="100%" data-style="btn-mint">
                        <option>2015</option>
                        <option>2016</option>
                        <option>2017</option>
                    </select>
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-3 control-label" for="demo-hor-inputcanvass">Base de Datos:</label>
                <div class="col-md-2">
                    <label class="form-radio form-normal form-mint form-text"><input type="radio" class="chkDB" name="db" value="YP" checked>YP</label>
                    <label class="form-radio form-normal form-mint form-text"><input type="radio" class="chkDB" name="db" value="BR">BR</label>
                </div>
            </div>

        </div>
        <div class="panel-footer text-right">
            <button id="btnSearch" class="btn btn-mint btn-hover-mint add-tooltip" data-placement="top" data-toggle="panel-overlay" data-target="#demo-panel-ref" data-original-title="Buscar Transacciones">Buscar</button>
        </div>
    </div>
