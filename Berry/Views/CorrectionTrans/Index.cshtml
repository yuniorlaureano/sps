<script src="@Url.Content("~/plugins/bootbox/bootbox.min.js")"></script>
<script src="@Url.Content("~/js/demo/ui-modals.js")"></script>

<script src="@Url.Content("~/plugins/bootstrap-timepicker/bootstrap-timepicker.min.js")"></script>

<script src="@Url.Content("~/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js")"></script>

@*<script src="@Url.Content("~/js/moment/moment.min.js")"></script>*@
