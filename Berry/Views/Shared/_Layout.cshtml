        //    $.get($('li[data-name="Axesa"]').data('url'), function (fullAccess) {
        //        if (fullAccess == false) {
        //            $('li[data-name="Axesa"] ul').children().not('li[data-name="SalesReport"]').html('');
        //        }
        //    });
        //});

    </script>

    @RenderSection("scripts", false)



</body>
</html>
